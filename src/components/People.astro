---
import Person from '@components/Person.astro';

interface Props {
  people: {
    name: string;
    creds?: string[];
    pronouns?: string;
    roles?: string[];
    email: string;
    bio: string;
    skills: string[];
    socials?: {
      url?: string;
      name?: string;
    }[];
  }[];
}

const { people } = Astro.props;
---

{
  people.map((person) => (
    <Person {person} />
  ))
}